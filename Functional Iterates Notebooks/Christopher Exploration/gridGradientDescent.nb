Cell[BoxData[
 RowBox[{
  RowBox[{"gridDescentMinimize", "[", 
   RowBox[{"loss_", ",", " ", "varsAndBounds_", ",", " ", 
    RowBox[{"iters_", ":", "20"}], ",", " ", 
    RowBox[{"tempInput_", ":", "None"}], ",", " ", 
    RowBox[{"d_", ":", "0.0001"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"temp", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"tempInput", "==", "None"}], ",", " ", 
         RowBox[{
          RowBox[{"Mean", "[", 
           RowBox[{"varsAndBounds", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", " ", "4"}], "]"}], "]"}], "]"}], "/", "10"}],
          ",", "tempInput"}], "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"indices", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Table", "@@", 
          RowBox[{"Prepend", "[", 
           RowBox[{"varsAndBounds", ",", " ", 
            RowBox[{"varsAndBounds", "[", 
             RowBox[{"[", 
              RowBox[{"All", ",", " ", "1"}], "]"}], "]"}]}], "]"}]}], ",", 
         " ", 
         RowBox[{
          RowBox[{"Length", "[", "varsAndBounds", "]"}], "-", "1"}]}], 
