      RowBox[{"progress", "=", 
       RowBox[{"PrintTemporary", "[", "\"\<0% Complete\>\"", "]"}]}]}], "}"}],
     ",", " ", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Table", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"gradients", "=", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"loss", "@@", 
               RowBox[{"(", 
                RowBox[{"#", "+", 
                 RowBox[{"d", "*", 
                  RowBox[{"IdentityMatrix", "[", 
                   RowBox[{"Length", "[", "varsAndBounds", "]"}], "]"}]}]}], 
                ")"}]}], "-", 
              RowBox[{"loss", "@@", "#"}]}], ")"}], "/", "d"}], "&"}], "/@", 
          "indices"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"indices", "=", 
         RowBox[{"indices", "-", 
          RowBox[{"temp", "*", "gradients"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"indices", "=", 
