Cell["\<\
Helper function for overloading. For some reason, WL doesn\[CloseCurlyQuote]t \
treat all functions as having a head of Function. Don\[CloseCurlyQuote]t ask \
how it works.\
\>", "CodeText",
 CellChangeTimes->{{3.9394948796200314`*^9, 3.9394949169045124`*^9}, {
  3.9394949884705296`*^9, 3.9394949885891914`*^9}, {3.939499644580187*^9, 
  3.939499678812805*^9}},ExpressionUUID->"0ef4446a-1dd6-434c-b672-\
70f988109bf5"],

Cell[BoxData[
 RowBox[{
  RowBox[{"FunctionQ", "[", "f_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"Head", "[", "f", "]"}], "===", "Function"}], "||", 
   RowBox[{"!", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"Information", "[", 
          RowBox[{"f", ",", " ", "\"\<Definitions\>\""}], "]"}], "[", 
         RowBox[{"[", "0", "]"}], "]"}], "===", "Information"}], ")"}], "||", 
      RowBox[{
       RowBox[{"Information", "[", 
        RowBox[{"f", ",", " ", "\"\<Definitions\>\""}], "]"}], "===", 
       "None"}]}], ")"}]}], "||", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{"Quiet", "[", 
       RowBox[{
        RowBox[{"Context", "[", "f", "]"}], ",", 
        StyleBox[
         RowBox[{"Context", "::", "ssle"}], "MessageName"]}], "]"}], "===", 
      "\"\<System`\>\""}], "&&", 
     RowBox[{
      RowBox[{"DownValues", "[", "f", "]"}], "===", 
      RowBox[{"{", "}"}]}]}], ")"}]}]}]], "Input",
 CellChangeTimes->{{3.9394612486732197`*^9, 3.9394612486732197`*^9}, {
   3.939461336646763*^9, 3.9394613430093613`*^9}, {3.9394613790090714`*^9, 
   3.9394613850443134`*^9}, {3.9394614520407734`*^9, 
   3.9394615430669575`*^9}, {3.93946832522937*^9, 3.9394683526793785`*^9}, 
   3.9394959890112305`*^9, 3.939499641489767*^9},
 CellLabel->"In[1]:=",ExpressionUUID->"1c106d1d-8164-274d-b31c-b32d695f768a"],

Cell["\<\
Overloading Composition (i.e. @* operator) to do repeated composition\
\>", "CodeText",
 CellChangeTimes->{{3.9394949222803097`*^9, 3.939494948728796*^9}, {
  3.939503972006279*^9, 
  3.9395039881833305`*^9}},ExpressionUUID->"98bcf349-84a9-794e-be3a-\
5869396f5914"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Unprotect", "[", "Composition", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Composition", "[", 
   RowBox[{
    RowBox[{"f_", "?", "FunctionQ"}], ",", " ", 
    RowBox[{"n_Integer", "?", "Positive"}]}], "]"}], ":=", 
  RowBox[{"Nest", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "@*", "#"}], "&"}], ",", "f", ",", " ", 
    RowBox[{"n", "-", "1"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Composition", "[", 
   RowBox[{
    RowBox[{"f_", "?", "FunctionQ"}], ",", " ", "0"}], "]"}], ":=", 
  RowBox[{"#", "&"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Protect", "[", "Composition", "]"}], ";"}]}], "Input",
 CellChangeTimes->{{3.9394603711272373`*^9, 3.939460371736429*^9}, {
   3.9394604921800613`*^9, 3.9394605382644825`*^9}, {3.939460588086685*^9, 
   3.9394606265823555`*^9}, {3.939460810722826*^9, 3.939460837151577*^9}, {
