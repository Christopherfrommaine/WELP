   3.9394618128649387`*^9}, {3.939466879473398*^9, 3.9394669747223816`*^9}, {
   3.9394682781718063`*^9, 3.9394683058602962`*^9}, {3.9394683427845726`*^9, 
   3.939468349662216*^9}, {3.939494956178707*^9, 3.939494962959385*^9}, {
   3.939495084748955*^9, 3.939495144515629*^9}, {3.9394958595016136`*^9, 
   3.939495937111397*^9}, 3.939496303649029*^9, {3.9394963375690536`*^9, 
   3.9394963723344707`*^9}, {3.939503948872244*^9, 3.9395039976749268`*^9}},
